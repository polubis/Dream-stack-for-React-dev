  "scripts": {
    "prepare": "cd .. && husky install system/.husky",
    "lint:checkAll": "npx nx run-many --target=lint --all",
    "lint:fixAll": "npx nx run-many --target=lint --all --fix",
    "format:checkAll": "prettier --list-different \"./**/*.{js,ts,tsx}\"",
    "format:fixAll": "prettier --write \"./**/*.{js,ts,tsx}\"",
    "test:checkAll": "npx nx run-many --target=test --all",
    "ts:checkAll": "npx nx run-many --target=type-check --all",
    "build-storybook": "npx nx run figa-ui:build-storybook -s public",
    "storybook:figa-ui": "npx nx run figa-ui:storybook",
    "test:figa-ui": "npx nx run figa-ui:test --watch",
    "test:jamjam": "npx nx run jamjam:test --watch",
    "nx:cloud-update": "npx nx g @nrwl/nx-cloud:init",
    "e2e:jamjam": "npx nx run jamjam-e2e:e2e --watch",
    "e2e:checkAll": "npx nx run-many --target=e2e --all --parallel",
    "chromatic": "npx run chromatic"
  },
